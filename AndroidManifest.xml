<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.contractortracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.contractortracker.Settings"
            android:label="Settings" />
        <activity
            android:name="com.example.contractortracker.Maintenance"
            android:label="Maintenance" />
        <activity
            android:name="com.example.contractortracker.Fuel"
            android:label="Fuel" />
        <activity
            android:name="com.example.contractortracker.Meals"
            android:label="Meals" />
        <activity
            android:name="com.example.contractortracker.Time"
            android:label="Time" />
        <activity
            android:name="com.example.contractortracker.Equipment"
            android:label="Equipment" />
        <activity
            android:name="com.example.contractortracker.Mileage"
            android:label="Mileage" />
    </application>

</manifest>